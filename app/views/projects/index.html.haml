%table.projects
  %tr
    %th= t('project.name')
    %th= t('project.description')
    %th
  - @projects.each do |project|
    %tr
      %td= project.name
      %td= project.description
      %td= link_to(t('edit'), edit_project_path(project))
.menu
  =link_to(t('create_new_project'), new_project_path)

