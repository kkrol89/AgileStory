.board{:class => "#{board.type.downcase} #{class_for_managed(current_user, @project)}", 'data-board-id' => board.id}
  .name= board.name
  = render :partial => 'project/sprints/management', :locals => { :sprint => board } if board.sprint?
  .tickets
    %ol
      = render :partial => 'project/tickets/ticket', :collection => board.tickets